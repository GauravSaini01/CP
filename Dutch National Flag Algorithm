#include<iostream>
using namespace std;
void dutchNationalFlagAlgo(int a[],int n){
	int low=0,mid=0,high=n-1;
	while(mid<=high){
		if (a[mid]==0){
			swap(a[low],a[mid]);
			low++;
			mid++;
		}else{
			if (a[mid]==1)
			{
			  	mid++;
			}else{
				swap(a[mid],a[high]);
				high--;
			}
			}
		}
}
int main(){
	int n;
	cin>>n;
	int a[n];
	for (int i = 0; i < n; ++i)
	{
		cin>>a[i];
	}
	dutchNationalFlagAlgo(a,n);
	for (int i = 0; i < n; ++i)
	{
		cout<<a[i]<<" ";
	}
	return 0;
}
